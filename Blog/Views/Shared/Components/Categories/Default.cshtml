@model List<Category>

<ol class="list-unstyled mb-0">
    <li>
        <a asp-controller="Home" asp-action="Index">All Text</a>
    </li>
    @foreach (var category in Model)
    {
        <li>
            <a asp-controller="Home" asp-action="Index" asp-route-categoryId="@category.Id">@category.Name</a>
        </li>
    }
</ol>